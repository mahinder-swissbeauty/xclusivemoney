    <script>
      document.addEventListener('DOMContentLoaded', function () {
        const menuBtn = document.getElementById('menuToggle');
        const menu = document.getElementById('dropdownMenu');
        const header = document.getElementById('siteHeader');

        // Mobile menu toggle
        if (menuBtn && menu) {
          menuBtn.addEventListener('click', () => {
            menu.classList.toggle('xcl-hidden');
          });

          // Click outside to close
          document.addEventListener('click', (e) => {
            const clickedInsideMenu = menu.contains(e.target);
            const clickedMenuBtn = menuBtn.contains(e.target);

            if (!clickedInsideMenu && !clickedMenuBtn) {
              menu.classList.add('xcl-hidden');
            }
          });
        }

        // Smooth sticky effect on scroll
        if (header) {
          window.addEventListener('scroll', () => {
            if (window.scrollY > 10) {
              header.classList.add('is-scrolled');
            } else {
              header.classList.remove('is-scrolled');
            }
          });
        }

        // Scroll links with offset + close menu on click



     document.querySelectorAll('a.scroll-link').forEach((link) => {
      link.addEventListener('click', function (e) {
        e.preventDefault();

        const href = this.getAttribute('href');
        const target = href ? document.querySelector(href) : null;
        if (!target) return;

        // ✅ close menu (same behavior)
        if (typeof menu !== "undefined" && menu && !menu.classList.contains('xcl-hidden')) {
          menu.classList.add('xcl-hidden');
        }

        const headerEl = document.querySelector('#siteHeader');

        // ✅ IMPORTANT FIX:
        // If we are near top (0-15px), header is not "scrolled" yet,
        // but during scroll it becomes "is-scrolled" (shrinks).
        // So we force it BEFORE measuring offset to avoid extra gap.
        if (headerEl && window.scrollY > 0 && window.scrollY <= 15) {
          headerEl.classList.add('is-scrolled');
        }

        const getOffset = () => {
          if (!headerEl) return 120;

          // Use visible bottom (covers sticky header + any top bar inside it)
          const r = headerEl.getBoundingClientRect();
          const cover = Math.max(0, r.bottom);

          return Math.ceil(cover + 24); // small safe gap
        };

        // ✅ Measure after DOM applies header class (no blink, single scroll)
        requestAnimationFrame(() => {
          requestAnimationFrame(() => {
            const offset = getOffset();
            const topPos = target.getBoundingClientRect().top + window.pageYOffset;
            const finalTop = Math.max(0, topPos - offset);

            window.scrollTo({ top: finalTop, behavior: 'smooth' });
          });
        });
      });
    });

        
      });
    </script>

    <script>
      (function () {
        // If user previously went to checkout from storefront, clear cart once when they return
        var shouldClear = sessionStorage.getItem('xcl_clear_cart_on_return') === '1';
        if (!shouldClear) return;

        // run once
        sessionStorage.removeItem('xcl_clear_cart_on_return');

        fetch('/cart/clear.js', { method: 'POST' })
          .then(function () {
            /* optional: update UI */
          })
          .catch(function () {});
      })();
    </script>

    <script>
      (function () {
        function resetCheckoutButtons() {
          document.querySelectorAll('.js-checkout-form').forEach((form) => {
            const btn = form.querySelector('.js-checkout-btn');
            const text = form.querySelector('.js-btn-text');
            const loader = form.querySelector('.js-btn-loader');

            if (btn) {
              btn.disabled = false;
              btn.style.pointerEvents = 'auto';
            }
            if (text) text.classList.remove('xcl-hidden');
            if (loader) loader.classList.add('xcl-hidden');
          });
        }

        // Normal load
        document.addEventListener('DOMContentLoaded', resetCheckoutButtons);

        // ✅ When coming back from checkout using browser back (BFCache)
        window.addEventListener('pageshow', function (e) {
          // e.persisted = page restored from bfcache
          if (e.persisted) resetCheckoutButtons();
          // also safe to reset anyway:
          resetCheckoutButtons();
        });
      })();
    </script>

    <script>
      (function () {
        function resetLoadingState(root) {
          (root || document).querySelectorAll('.js-checkout-form').forEach((form) => {
            const btn = form.querySelector('.js-checkout-btn');
            const text = form.querySelector('.js-btn-text');
            const loader = form.querySelector('.js-btn-loader');

            if (btn) {
              btn.disabled = false;
              btn.style.pointerEvents = 'auto';
            }
            if (text) text.classList.remove('xcl-hidden');
            if (loader) loader.classList.add('xcl-hidden');
          });
        }

        // ✅ reset immediately on normal load
        document.addEventListener('DOMContentLoaded', () => resetLoadingState());

        // ✅ when coming back (BFCache restore) - very common on mobile
        window.addEventListener('pageshow', () => resetLoadingState());

        // ✅ when tab becomes visible again
        document.addEventListener('visibilitychange', () => {
          if (document.visibilityState === 'visible') resetLoadingState();
        });

        // ✅ when window regains focus (mobile back)
        window.addEventListener('focus', () => resetLoadingState());

        // ✅ IMPORTANT: before leaving page, clear loading (prevents cached loading UI)
        window.addEventListener('pagehide', () => resetLoadingState());
      })();
    </script>
      <script src = "https://checkout.razorpay.com/v1/checkout.js"></script>
