{% comment %}
  Section: TradeEdge Pro Comparison (Desktop layout tuned)

  Product metafield:
  - Namespace/key: custom.tradeedge_comparison_block
  - Type: Metaobject reference (single) to: tradeedge_comparison_block

  Suggested metaobject fields:
  - without_title      : text
  - with_title         : text
  - without_point_1-4  : text / textarea
  - with_point_1-4     : text / textarea
  - left_avatar_image  : image
  - left_avatar_alt    : text
  - right_avatar_image : image
  - right_avatar_alt   : text
{% endcomment %}

{% liquid
  assign comp_meta = nil

  if product and product.metafields.custom.tradeedge_comparison_block
    assign comp_meta = product.metafields.custom.tradeedge_comparison_block.value
  endif

  comment
    Base values from section settings (fallback)
  endcomment
  assign without_title  = section.settings.without_title
  assign with_title     = section.settings.with_title

  assign without_point_1 = section.settings.without_point_1
  assign without_point_2 = section.settings.without_point_2
  assign without_point_3 = section.settings.without_point_3
  assign without_point_4 = section.settings.without_point_4

  assign with_point_1 = section.settings.with_point_1
  assign with_point_2 = section.settings.with_point_2
  assign with_point_3 = section.settings.with_point_3
  assign with_point_4 = section.settings.with_point_4

  assign left_avatar_image  = section.settings.left_avatar_image
  assign left_avatar_alt    = section.settings.left_avatar_alt
  assign right_avatar_image = section.settings.right_avatar_image
  assign right_avatar_alt   = section.settings.right_avatar_alt

  if comp_meta
    if comp_meta.without_title != blank
      assign without_title = comp_meta.without_title
    endif

    if comp_meta.with_title != blank
      assign with_title = comp_meta.with_title
    endif

    if comp_meta.without_point_1 != blank
      assign without_point_1 = comp_meta.without_point_1
    endif
    if comp_meta.without_point_2 != blank
      assign without_point_2 = comp_meta.without_point_2
    endif
    if comp_meta.without_point_3 != blank
      assign without_point_3 = comp_meta.without_point_3
    endif
    if comp_meta.without_point_4 != blank
      assign without_point_4 = comp_meta.without_point_4
    endif

    if comp_meta.with_point_1 != blank
      assign with_point_1 = comp_meta.with_point_1
    endif
    if comp_meta.with_point_2 != blank
      assign with_point_2 = comp_meta.with_point_2
    endif
    if comp_meta.with_point_3 != blank
      assign with_point_3 = comp_meta.with_point_3
    endif
    if comp_meta.with_point_4 != blank
      assign with_point_4 = comp_meta.with_point_4
    endif

    if comp_meta.left_avatar_image != blank
      assign left_avatar_image = comp_meta.left_avatar_image
    endif
    if comp_meta.left_avatar_alt != blank
      assign left_avatar_alt = comp_meta.left_avatar_alt
    endif

    if comp_meta.right_avatar_image != blank
      assign right_avatar_image = comp_meta.right_avatar_image
    endif
    if comp_meta.right_avatar_alt != blank
      assign right_avatar_alt = comp_meta.right_avatar_alt
    endif
  endif

  comment
    Decide if section has any content or not
  endcomment
  assign has_content = false

  if without_title != blank
    assign has_content = true
  elsif with_title != blank
    assign has_content = true
  elsif without_point_1 != blank or without_point_2 != blank or without_point_3 != blank or without_point_4 != blank
    assign has_content = true
  elsif with_point_1 != blank or with_point_2 != blank or with_point_3 != blank or with_point_4 != blank
    assign has_content = true
  elsif left_avatar_image or right_avatar_image
    assign has_content = true
  endif
%}

{% if has_content %}

