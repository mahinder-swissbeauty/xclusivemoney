  id="{{ section.id }}-tradeedge-comparison"
  class="sw-bg-[#151515] sw-text-white sw-py-10 sw-px-4 xl:sw-py-16"
  style="background-color: {{ section.settings.section_bg }}; color: {{ section.settings.section_text_color }};"
>

  <!-- MAIN HEADING -->
  {% render 'section-heading' %}

  <!-- TWO COLUMNS -->
  <div class="tradeedge-wrap sw-max-w-[360px] xl:sw-max-w-[1080px] sw-mx-auto sw-grid sw-gap-8 xl:sw-gap-[120px] xl:sw-grid-cols-[1fr,1fr]">

    {%- comment -%} LEFT: WITHOUT TRADEDGE PRO {%- endcomment -%}
    <div class="sw-grid sw-grid-cols-[88px,1fr] sw-gap-6 xl:sw-grid-cols-[120px,1fr] xl:sw-gap-10">
      <!-- Avatar -->
      <div class="sw-flex-shrink-0 sw-mt-auto">
        {% if left_avatar_image %}
          <img
            src="{{ left_avatar_image | image_url: width: 220 }}"
            alt="{{ left_avatar_alt | default: without_title }}"
            class="sw-w-full sw-h-auto sw-object-contain"
            loading="lazy"
